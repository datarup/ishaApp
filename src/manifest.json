{
"name": "Isha App",
"version": "1.0",
"description": "Shows product list",
"permissions": [
  "identity",
  "background",
  "alarms",
  "tabs",
  "http://*/",
  "https://chromeapp.vendhq.com/api/products?page_size=2&page=1&active=true",
  "https://chromeapp.vendhq.com/api/products?*",
  "https://ishashoppe.vendhq.com/api/products?page_size=2&page=1&active=true",
  "https://ishashoppe.vendhq.com/api/products?*",
  "https://www.google.com/accounts/OAuthGetAccessToken"
],

"background": { "scripts": ["background.js"]},

"browser_action": {
  "default_icon": "assets/images/isha19.png",
  "default_title": "Isha App"
//  "default_popup": "index.html"
},

"icons": {
  "16": "assets/images/isha16.png",
  "48": "assets/images/isha48.png",
  "128": "assets/images/isha128.png"
},

"manifest_version": 2
}
